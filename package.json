{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "format:check": "prettier --config .prettierrc --check \"src/*.*\"",
    "format:fix": "prettier --config .prettierrc --write \"src/*.*\"",
    "lint:check": "eslint .",
    "knip:check": "knip",
    "type:check": "npm run type:check:app && npm run type:check:test",
    "type:check:app": "tsc --project tsconfig.app.json --noEmit",
    "type:check:test": "tsc --project tsconfig.test.json --noEmit",
    "preview": "vite preview",
    "test:unit": "vitest --project unit --run",
    "test:unit:watch": "vitest --project unit",
    "test:component": "vitest --project component --run",
    "test:component:watch": "vitest --project component",
    "test:integration": "vitest --project integration --run",
    "test:integration:watch": "vitest --project integration",
    "test:all": "npm run test:unit && npm run test:component && npm run test:integration",
    "test:coverage": "vitest --run --coverage"
  },
  "dependencies": {
    "@faker-js/faker": "^10.1.0",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/vite": "^4.1.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.552.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.66.0",
    "react-resizable-panels": "^3.0.6",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.17",
    "zod": "^4.1.12",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "4.0.14",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "happy-dom": "^20.0.10",
    "knip": "^5.69.1",
    "prettier": "^3.7.3",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.6",
    "vitest": "^4.0.8"
  }
}
